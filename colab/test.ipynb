    "\n",
    "\n",
    "def print_current_time():\n",
    "    print(datetime.datetime.now().strftime('%Y/%m/%d %H:%M:%S'))\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    main()\n"
